{
  "$schema-template": "2.0.0",
  "modulesContent": {
    "$edgeAgent": {
      "properties.desired": {
        "schemaVersion": "1.1",
        "runtime": {
          "type": "docker",
          "settings": {
            "minDockerVersion": "v1.25",
            "loggingOptions": "",
            "registryCredentials": {
              "raaedge": {
                "username": "${ACR_USER}",
                "password": "${ACR_PASSWORD}",
                "address": "${ACR_ADDRESS}"
              }
            }
          }
        },
        "modules": {
          "HealthMonitor": {
            "version": "1.0",
            "type": "docker",
            "status": "running",
            "restartPolicy": "always",
            "startupOrder": 2,
            "settings": {
              "image": "raaedge.azurecr.io/connectors-healthmonitor:2.1.3",
              "createOptions": {
                "HostConfig": {
                  "Binds": [
                    "/etc/iotedge/storage/edgeHub:/app/data"
                  ]
                }
              }
            }
          }
        }
      }
    },
    "$edgeHub": {
      "properties.desired": {
        "schemaVersion": "1.1",
        "routes": {
          "HealthmonitorToIdentityMapper": "FROM /messages/modules/HealthMonitor/outputs/* INTO BrokeredEndpoint(\"/modules/IdentityMapper/inputs/events\")"
        },
        "storeAndForwardConfiguration": {
          "timeToLiveSecs": 7200
        }
      }
    }
  }
}